
<%#= simple_form_for @ticketnote, validate: true do |f| %>

<%= simple_form_for @ticketnote do |f| %>

  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :troubleticket_id %>

    <%= f.input :notes %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
