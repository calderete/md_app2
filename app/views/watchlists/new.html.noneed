<% @user = current_user %>
<% if user_signed_in? %>


<h1>New watch list item</h1>

<%= render 'form', watchlist: @watchlist %>

<%= link_to 'Back', watchlists_path %>


<% else %>
  <div class="alert alert-warning" role="alert"> You must be signed in administrator to add a new watch list item.</div>
<% end %>
