<!-- Find me in app/views/watchlists/_form -->

<%= simple_form_for @watchlist do |f| %>


  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id , :readonly => true ,input_html: { value: @current_user.id } %>

    <%= f.input :product_id %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

<!-- end -->
